@{
    ViewData["Title"] = "Home Page";
}


<!-- =================================
            ///// Begin intro static section /////
            ================================== -->
<section id="intro-static-section">
    <div class="section-inner no-padding full-height bg-image" style="background-image: url(/images/memphis_city.jpg); background-position: 50% 75%;">

        <div class="intro-static-caption text-white text-center align-center max-width-800">
            <h1 class="intro-static-title">make your business visible</h1>

            <p class="intro-static-description">strategies for your online presence</p>
            <a href="#about" class="btn btn-primary margin-top-20 margin-right-5 sm-scroll">read more</a>
            <a href="#contact-section" class="btn btn-white-bordered margin-top-20 sm-scroll">contact us</a>
        </div>

        <!-- Element cover -->
        <div class="cover bg-transparent-5-dark"></div>

        <!-- Scroll down arrow -->
        <a class="scroll-down-arrow sm-scroll text-center" href="#about"><span><i class="fa fa-arrow-down"></i></span></a>

    </div> <!-- /.section-inner -->
</section>
<!-- End intro static section -->








<!-- ============================
            ///// Begin about section /////
            ============================= -->

